Options +FollowSymlinks -MultiViews -Indexes

RewriteEngine On
RewriteBase /

RewriteCond %{THE_REQUEST} ^[A-Z]{3,}\s/+php/([^\s]+) [NC]
RewriteRule ^ %1 [R=301,L]

RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule (?!^php/)^(.*)$ /php/$1 [L,NC]

ErrorDocument 404 http://www.atarilegend.com/themes/templates/1/page_not_found.html
ErrorDocument 403 http://www.atarilegend.com/themes/templates/1/page_not_found.html

AuthName "Atari Legend"
AuthType Basic
AuthUserFile "/homepages/16/d691186105/htdocs/.htpasswd"
Require valid-user
